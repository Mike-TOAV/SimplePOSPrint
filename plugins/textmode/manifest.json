{
  "manifest_version": 3,
  "name": "SimplePOSPrint",
  "version": "1.0",
  "description": "Redirect POS receipts to thermal printer",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_idle"
    }
  ]
}
